{
    "exp": {
      "name": "cvae_qz_bbh"
    },
    "data_loader": {
      "name": "GWcVAEDataLoader",
      "args": {},
      "data_path": "/home/nino/GW/data/bbh_qz_data.hdf5",
      "scale_data": false,
      "data_output_type": "hp",
      "split_size": 0.2,
      "sxs_sample_weight": 50 
    },
    "model":{
      "name": "cVAEGenerator",
      "args": {},
      "latent_dim": 10,
      "reg_weight": 0.01,
      "optimizer_kwargs":{
        "learning_rate": 0.0001
      }
    },
    "trainer":{
      "name": "GWModelTrainer",
      "args": {},
      "num_epochs": 1500,
      "mapper_epochs": 1500,
      "generator_epochs": 1500,
      "batch_size": 50,
      "batch_size_test": 1024,
      "verbose_training": 2
    },
    "callbacks":{
      "checkpoint_monitor": "loss",
      "checkpoint_mode": "min",
      "checkpoint_save_best_only": true,
      "checkpoint_save_weights_only": true,
      "checkpoint_verbose": true,
      "tensorboard_write_graph": true,
      "early_stopping_patience": 55,
      "lr_reduce_factor": 0.5,
      "lr_reduce_patience": 25,
      "min_lr": 5e-6
    }
  }
